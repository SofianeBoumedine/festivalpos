{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es5",
        "sourceMap": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "skipDefaultLibCheck": true,
        "strict": true,
        "lib": [
            "es2017",
            "dom"
        ],
        "noImplicitAny": false
    },
    "exclude": [
        "bin",
        "node_modules"
    ]
}